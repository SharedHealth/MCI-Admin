    <h4>Created By: {{ createdBy}}</h4>
    <h4>Created At: {{ createdAt|date}}</h4>
    {% if responseBody is defined and responseBody is not empty %}
    {% for result in responseBody %}
    <p>{{ result.event_time|date('Y-M-d H:i:s') }}</p>
    <table class="table table-bordered">

         <tr>
             <th>Field</th>
             <th>Old Value</th>
             <th>New Value</th>
             <th>Requested By</th>
             <th>Approved By</th>
         </tr>

     {% for key, change in result.change_set  %}
      <tr>
       <td>
           <p>{% if key == 'sur_name' %}
                {{ 'Surname' }}
                 {% else %}
                     {{ key|camelize }}
                 {% endif %}
           </p>
       </td>
       <td>
              {% if change.old_value is iterable %}
                  {% for old_value_key, old_value_details in change.old_value  %}
                      {% set value_details = old_value_details is iterable ? old_value_details|json_encode:old_value_details %}
                      {{ old_value_key|camelize }} : {{ value_details }}<br/>
                  {% endfor %}
              {% else %}
                  {{ change.old_value  }}
              {% endif %}
       </td>
       <td>
         {% if change.new_value is iterable %}
             {% for new_value_key,new_value_details in change.new_value  %}
                 {% set value_details = new_value_details is iterable ? new_value_details|json_encode:new_value_details %}
                 {{ new_value_key|camelize }} : {{ value_details }}<br/>
             {% endfor %}
         {% else %}
          {{ change.new_value  }}
         {% endif %}
       </td>
       <td>

           {% for requestedBy in result.requested_by %}
                {{ requestedBy|join('-') }}
           {% endfor %}
       </td>
       <td>{{ result.approved_by }}</td>

      </tr>
     {% endfor %}

    </table>
{% endfor %}
{% endif %}

<div style="clear: both"></div>